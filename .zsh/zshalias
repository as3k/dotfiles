## location aliases
alias udv='cd ~/www/p/udemy-vue'
alias pv2='cd ~/www/p/portfolio/wp-content/themes/pv2'

## unix aliases
alias c='clear'
alias e='$EDITOR'
alias lA='ls -A'
alias less='gulp less'
alias ln='ln -v'
alias ll='ls -al'
alias lp='ls -ap'
alias lvserv='php artisan serv --host=104.236.201.11 --port=8000'
alias mkdir='mkdir -p'
alias rollup='gulp rollup'
alias v='$VISUAL'
alias zs='source ~/.zshrc'
alias zsync='rsync -u -v ~/.zshrc ~/.shell/.zshrc'

## application aliases
alias gulp='sudo gulp'

## git specific aliases
alias gi='git init'
alias gpsu='git push --set-upstream'
